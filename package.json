{
  "name": "kuali-dms",
  "version": "0.1.0",
  "bin": {
    "kuali-dms": "bin/kuali-dms.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth --all 2>&1 | tee cdk.out/kuali-dms.template.yaml",
    "deploy": "cdk deploy --all --no-rollback --require-approval never",
    "destroy": "cdk destroy -f --all --no-rollback --require-approval never",
    "redeploy": "npm run destroy && sleep 60 && npm run deploy"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.1006.0",
    "esbuild": "^0.25.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.876.0",
    "@aws-sdk/client-database-migration-service": "^3.846.0",
    "@aws-sdk/client-iam": "^3.873.0",
    "@aws-sdk/client-rds": "^3.876.0",
    "@aws-sdk/client-scheduler": "^3.883.0",
    "aws-cdk-lib": "^2.214.0",
    "constructs": "^10.0.0",
    "dotenv": "^17.2.0",
    "humanize-duration": "^3.33.0",
    "uuid": "^12.0.0"
  }
}
